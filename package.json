{
  "name": "update-it",
  "version": "0.1.1",
  "description": "Functions to update javascript object tree.",
  "main": "lib/index.js",
  "scripts": {
    "start": "node devserver.js",
    "lint": "eslint --ext .js src",
    "babel": "babel src --out-dir lib",
    "coverage": "babel-node -r node_modules/isparta/lib/cli cover --root src --include-all-sources --report lcov --report html node_modules/mocha/bin/_mocha -- test",
    "coverage-check": "node node_modules/isparta/node_modules/istanbul/lib/cli check-coverage --statements 100 --functions 100 --branches 100 --lines 100",
    "codecov": "cat ./coverage/coverage.json | ./node_modules/codecov.io/bin/codecov.io.js",
    "test": "npm run lint && npm run coverage && npm run codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergeyt/update-it.git"
  },
  "keywords": [
    "update",
    "patch",
    "helpers"
  ],
  "author": "Sergey Todyshev <stodyshev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sergeyt/update-it/issues"
  },
  "homepage": "https://github.com/sergeyt/update-it#readme",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "codecov.io": "^0.1.6",
    "deep-freeze-strict": "^1.1.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-plugin-react": "^3.16.0",
    "expect.js": "^0.3.1",
    "express": "^4.13.4",
    "isparta": "^4.0.0",
    "mocha": "^2.3.4",
    "webpack": "^1.12.12"
  },
  "dependencies": {
    "lodash": "^4.0.0"
  }
}
