{
  "name": "update-it",
  "version": "0.2.1",
  "description": "Functions to update javascript object tree.",
  "main": "lib/index.js",
  "scripts": {
    "start": "node devserver.js",
    "lint": "eslint src",
    "babel": "babel src --out-dir lib",
    "coverage": "babel-node -r node_modules/isparta/lib/cli cover --root src --include-all-sources --report json --report html node_modules/mocha/bin/_mocha -- test",
    "codecov": "cat ./coverage/coverage.json | ./node_modules/codecov.io/bin/codecov.io.js",
    "test": "npm run lint && npm run coverage && npm run codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergeyt/update-it.git"
  },
  "keywords": [
    "update",
    "patch",
    "helpers"
  ],
  "author": "Sergey Todyshev <stodyshev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sergeyt/update-it/issues"
  },
  "homepage": "https://github.com/sergeyt/update-it#readme",
  "devDependencies": {
    "codecov.io": "^0.1.6",
    "deep-freeze-strict": "^1.1.1",
    "expect.js": "^0.3.1",
    "isparta": "^4.0.0",
    "mocha": "^3.1.2",
    "react-devpack": "^0.2.0"
  },
  "dependencies": {
    "lodash": "^4.16.3"
  }
}
